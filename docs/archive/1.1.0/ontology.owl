    <!-- https://w3id.org/iadopt/ont/AsymmetricSystem -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/AsymmetricSystem">
        <rdfs:subClassOf rdf:resource="https://w3id.org/iadopt/ont/System"/>
        <rdfs:comment>A system of entities where each Entity acts in a specific role and roles are not mutually interchangeable.</rdfs:comment>
        <rdfs:label>Asymmetric System</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/Constraint -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/Constraint">
        <rdfs:comment>A Constraint limits the scope of the observation and confines the context to a particular state. It describes properties of the involved entities that are relevant to the particular observation.</rdfs:comment>
        <rdfs:label>Constraint</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/Entity -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/Entity">
        <rdfs:comment>An object or process that has a role in an observation. An Entity may play one of the following roles: ObjectOfInterest, ContextObject, or Matrix. Whether the involvement of a particular entity is meaningful enough to include in the variable description depends on the specific context.</rdfs:comment>
        <rdfs:label>Entity</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/Property -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/Property">
        <rdfs:comment>A type of a characteristic of the ObjectOfInterest.</rdfs:comment>
        <rdfs:label>Property</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/StatisticalModifier -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/StatisticalModifier">
        <rdfs:comment>The statistical modifier describes which statistical measure has been applied.</rdfs:comment>
        <rdfs:label>Statistical Modifier</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/SymmetricSystem -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/SymmetricSystem">
        <rdfs:subClassOf rdf:resource="https://w3id.org/iadopt/ont/System"/>
        <rdfs:comment>A system of entities where the roles of all entities are mutually interchangeable.</rdfs:comment>
        <rdfs:label>Symmetric System</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/System -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/System">
        <rdfs:subClassOf rdf:resource="https://w3id.org/iadopt/ont/Entity"/>
        <rdfs:comment>A combination of two or more entities.</rdfs:comment>
        <rdfs:label>System</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/iadopt/ont/Variable -->


    <owl:Class rdf:about="https://w3id.org/iadopt/ont/Variable">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/iadopt/ont/hasObjectOfInterest"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/iadopt/ont/Entity"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/iadopt/ont/hasProperty"/>
